<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aber.group9.pubapp">


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectLocationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PubListActivity" />
        <activity android:name=".EmptyTourActivity" />
        <activity android:name=".CongratulationActivity" />
        <activity android:name=".PubInfoActivity" />
        <activity android:name=".FilterActivity" />
        <activity android:name=".HomeActivity"/>
        <activity android:name=".CreateTourActivity" />
        <activity android:name=".ViewTourActivity"></activity>
    </application>

</manifest>